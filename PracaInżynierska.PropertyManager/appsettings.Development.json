{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.AspNetCore.SpaProxy": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
    "AllowedHosts": "*",
    "StaticFiles": {
      "Headers": {
        "CacheControl": "no-cache, no-store",
        "Pragma": "no-cache",
        "Expires": "-1"
      }
    }
  }
